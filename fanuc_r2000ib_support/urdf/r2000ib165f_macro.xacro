<?xml version="1.0" encoding="utf-8"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find fanuc_resources)/urdf/common_materials.xacro"/>

  <xacro:macro name="fanuc_r2000ib165f" params="prefix">
    <!-- link list -->
    <link name="${prefix}base_link">
      <visual>
        <origin xyz="0 0 0.135" rpy="0 0 0"/>
        <geometry>
          <box size="0.600 0.600 0.270"/>
        </geometry>
        <xacro:material_fanuc_gray28 />
      </visual>
      <collision>
        <origin xyz="0 0 0.135" rpy="0 0 0"/>
        <geometry>
          <box size="0.600 0.600 0.270"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_1">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_r2000ib_support/meshes/r2000ib165f/link_1.stl"/>
        </geometry>
        <xacro:material_fanuc_yellow />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_r2000ib_support/meshes/r2000ib165f/link_1.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_2">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_r2000ib_support/meshes/r2000ib165f/link_2.stl"/>
        </geometry>
        <xacro:material_fanuc_yellow />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_r2000ib_support/meshes/r2000ib165f/link_2.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_3">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_r2000ib_support/meshes/r2000ib165f/link_3.stl"/>
        </geometry>
        <xacro:material_fanuc_yellow />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_r2000ib_support/meshes/r2000ib165f/link_3.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_4">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_r2000ib_support/meshes/r2000ib165f/link_4.stl"/>
        </geometry>
        <xacro:material_fanuc_yellow />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_r2000ib_support/meshes/r2000ib165f/link_4.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_5">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_r2000ib_support/meshes/r2000ib165f/link_5.stl"/>
        </geometry>
        <xacro:material_fanuc_yellow />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_r2000ib_support/meshes/r2000ib165f/link_5.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_6">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_r2000ib_support/meshes/r2000ib165f/link_6.stl"/>
        </geometry>
        <xacro:material_fanuc_yellow />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_r2000ib_support/meshes/r2000ib165f/link_6.stl"/>
        </geometry>
      </collision>
    </link>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1" type="revolute">
      <origin xyz="0 0 0.670" rpy="0 ${pi} ${pi}" />
      <parent link="${prefix}base_link" />
      <child link="${prefix}link_1" />
      <axis xyz="0 0 1" />
      <limit lower="-${radians(180)}" upper="${radians(180)}" effort="0" velocity="${radians(95)}" />
    </joint>
    <joint name="${prefix}joint_2" type="revolute">
      <origin xyz="0.312 0 0" rpy="0 0 0" />
      <parent link="${prefix}link_1" />
      <child link="${prefix}link_2" />
      <axis xyz="0 1 0" />
      <limit lower="-${radians(60)}" upper="${radians(76)}" effort="0" velocity="${radians(90)}" />
    </joint>
    <joint name="${prefix}joint_3" type="revolute">
      <origin xyz="0 0 -1.075" rpy="0 0 0" />
      <parent link="${prefix}link_2" />
      <child link="${prefix}link_3" />
      <axis xyz="0 1 0" />
      <limit lower="-${radians(80)}" upper="${radians(200)}" effort="0" velocity="${radians(95)}" />
    </joint>
    <joint name="${prefix}joint_4" type="revolute">
      <origin xyz="1.280 0 -0.225" rpy="0 0 0" />
      <parent link="${prefix}link_3" />
      <child link="${prefix}link_4" />
      <axis xyz="1 0 0" />
      <limit lower="-${radians(360)}" upper="${radians(360)}" effort="0" velocity="${radians(120)}" />
    </joint>
    <joint name="${prefix}joint_5" type="revolute">
      <origin xyz="0 0.010 0" rpy="0 0 0" />
      <parent link="${prefix}link_4" />
      <child link="${prefix}link_5" />
      <axis xyz="0 1 0" />
      <limit lower="-${radians(125)}" upper="${radians(125)}" effort="0" velocity="${radians(120)}" />
    </joint>
    <joint name="${prefix}joint_6" type="revolute">
      <origin xyz="0.215 -0.010 0" rpy="0 ${pi/2} 0" />
      <parent link="${prefix}link_5" />
      <child link="${prefix}link_6" />
      <axis xyz="0 0 1" />
      <limit lower="-${radians(360)}" upper="${radians(360)}" effort="0" velocity="${radians(190)}" />
    </joint>
    <!-- end of joint list -->

    <!-- ROS-Industrial 'base' frame. base_link to Fanuc World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0.400" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>

    <!-- ROS-Industrial 'flange' frame. attachment point for EEF models -->
    <link name="${prefix}flange" />
    <joint name="${prefix}joint_6-flange" type="fixed">
      <origin xyz="0 0 0" rpy="${pi} -${pi/2} 0" />
      <parent link="${prefix}link_6" />
      <child link="${prefix}flange" />
    </joint>

    <!-- ROS-Industrial 'tool0' frame. all-zeros tool frame -->
    <link name="${prefix}tool0" />
    <joint name="${prefix}link_6-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="${pi} ${-pi/2} 0" />
      <parent link="${prefix}flange" />
      <child link="${prefix}tool0" />
    </joint>

  </xacro:macro>
</robot>